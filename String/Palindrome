#include <stdio.h>
 #include<string.h>
int main()
{
   
   int t;
   scanf("%d",&t);
   while(t--)
{
    char a[100],b[100];
    scanf("%s",a);
    int n=strlen(a);
    int j=0;
    for(int i=n-1;i>=0;i--)
          b[j++]=a[i];
       b[j]='\0';
       if(strcmp(a,b)==0)
       printf("YES\n");
       else
       printf("NO\n");
    
}
   return 0;
}
